<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mapScreenVM" type="com.tiagosapplication.app.modules.mapscreen.data.viewmodel.MapScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackradar"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnsearch"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameMapscreen"
                android:layout_height="@dimen/_559pxv"
                android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_332pxh">
                <LinearLayout android:id="@+id/linearColumnradar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <ImageView android:gravity="end"
                        android:id="@+id/imageRadar"
                        android:layout_height="@dimen/_274pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_radar"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_radar"/>
                    <FrameLayout android:id="@+id/frameNumberRadar"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_48pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_158pxv" android:layout_width="@dimen/_71pxh">
                        <ImageView android:id="@+id/imagePolygonTwo"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="@dimen/_44pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_polygon2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_polygon2"/>
                        <LinearLayout
                            android:id="@+id/linearColumnglobe"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylered_A700">
                            <ImageView android:id="@+id/imageGlobe"
                                android:layout_height="@dimen/_7pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_width="@dimen/_7pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_globe_black_900"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_globe_black_900"/>
                            <ImageView android:id="@+id/imageUpload"
                                android:layout_height="@dimen/_22pxh"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_upload"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_upload"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEightyNine"
                            android:layout_gravity="bottom|end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{mapScreenVM.mapScreenModel.txtEightyNine}"
                            style="@style/txtJetBrainsMonoromanregular24" tools:text="@string/lbl_89"/>
                    </FrameLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_114pxh"
                    android:layout_marginEnd="@dimen/_80pxh"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_114pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameBottombar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStyleblack_900">
                <LinearLayout android:id="@+id/linearRowglobeOne"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageGlobeOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_51pxh"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_globe"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_globe"/>
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_51pxh"
                        android:layout_marginStart="@dimen/_218pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageMaps"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_765pxv"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_maps}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_maps"/>
    </FrameLayout>
</layout>
